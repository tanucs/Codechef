#include <iostream>
using namespace std;

int main() {
    int T;
    cin>>T;
    while(T--){
        int X;
        cin>>X;
        if(X<7){
            cout<<"YES"<<endl;
        }
        else if(X==7){
            cout<<"NO"<<endl;
        }
        else if(X>7){
            cout<<"NO"<<endl;
        }
    }

	return 0;
}
