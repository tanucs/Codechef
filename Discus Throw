#include <iostream>
using namespace std;

int main() {
    int T;
    cin>>T;
    while(T--){
        int A,B,C;
        cin>>A>>B>>C;
        if(A>=B){
            cout<<A<<endl;
        }
        else if(C>=A){
            cout<<C<<endl;
        }
        else if(B>=C){
            cout<<B<<endl;
        }
    }

	return 0;
}
